  <script type="text/javascript">
    Array.prototype.slice.call(document.querySelectorAll('video')).forEach(video => {
      autoplay playsinline(video);
    })


    function autoplay playsinline(video) {
      let observer = new IntersectionObserver((entries, observer) => {
	entries.forEach(entry => {
	  console.log(video, entry.intersectionRatio)
          if(entry.intersectionRatio < 0.75 && !video.paused){
            video.pause();
          } else if(entry.intersectionRatio >= 0.75 && video.paused) {
            video.play();
	  }
        });
      }, {threshold: 0.75});

      observer.observe(video);
    }

</script>

